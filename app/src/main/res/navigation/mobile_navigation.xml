        tools:layout="@layout/activity_insert" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.pwmanager.ui.setting.SettingsFrag"
        android:label="Settings"
        tools:layout="@layout/activity_settings" />
